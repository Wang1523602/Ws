","toggleRowSelection","getList","params","JSON","parse","stringify","leixing","$http","url","method","then","res","data","total","Number","delClick","id","ids","length","x","push","ElMessageBox","confirm","confirmButtonText","cancelButtonText","type","$toolUtil","message","handleSelectionChange","e","layouts","sizeChange","size","currentChange","prevClick","nextClick","btnAuth","a","isAuth","searchClick","formRef","formModelChange","addClick","init","editClick","infoClick","preClick","file","window","open","$config","download","arr","replace","RegExp","axios","get","location","href","split","name","headers","token","storageGet","responseType","binaryData","objectUrl","URL","createObjectURL","Blob","document","createElement","dispatchEvent","MouseEvent","bubbles","cancelable","view","revokeObjectURL"],"sources":["E:/桌面/ylylxt/manage_code/src/views/leixing/list.vue"],"sourcesContent":["\r\n<template>\r\n\t<div>\r\n\t\t<div class=\"app-contain\">\r\n\t\t\t<div class=\"list_search_view\">\r\n\t\t\t\t<el-form :model=\"searchQuery\" class=\"search_form\" >\r\n\t\t\t\t\t<div class=\"search_view\">\r\n\t\t\t\t\t\t<div class=\"search_label\">\r\n\t\t\t\t\t\t\t类型：\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t<div class=\"search_box\">\r\n\t\t\t\t\t\t\t<el-input class=\"search_inp\" v-model=\"searchQuery.leixing\" placeholder=\"类型\"\r\n\t\t\t\t\t\t\t\tclearable>\r\n\t\t\t\t\t\t\t</el-input>\r\n\t\t\t\t\t\t</div>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t\t<div class=\"search_btn_view\">\r\n\t\t\t\t\t\t<el-button class=\"search_btn\" type=\"primary\" @click=\"searchClick()\" size=\"small\">搜索</el-button>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</el-form>\r\n\t\t\t\t<br>\r\n\t\t\t\t<div class=\"btn_view\">\r\n\t\t\t\t\t<el-button type=\"success\" @click=\"addClick\" v-if=\"btnAuth('leixing','新增')\">新增</el-button>\r\n\t\t\t\t\t<el-button  v-if=\" btnAuth('leixing','查看')\" type=\"info\"  :disabled=\"selRows.length==1?false:true\" @click=\"infoClick(null)\">详情</el-button>\r\n\t\t\t\t\t<el-button type=\"primary\" :disabled=\"selRows.length==1?false:true\" @click=\"editClick\" v-if=\" btnAuth('leixing','修改')\">修改</el-button>\r\n\t\t\t\t\t<el-button type=\"danger\" :disabled=\"selRows.length?false:true\" @click=\"delClick(null)\"  v-if=\"btnAuth('leixing','删除')\">删除</el-button>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t\t<br>\r\n\t\t\t<el-table\r\n\t\t\t\tv-loading=\"listLoading\"\r\n\t\t\t\tborder \r\n\t\t\t\t:stripe='false'\r\n\t\t\t\t@selection-change=\"handleSelectionChange\" \r\n\t\t\t\tref=\"table\"\r\n\t\t\t\tv-if=\"btnAuth('leixing','查看')\"\r\n\t\t\t\t:data=\"list\"\r\n\t\t\t\t@row-click=\"listChange\">\r\n\t\t\t\t<el-table-column :resizable='true' align=\"left\" header-align=\"left\" type=\"selection\" width=\"55\" />\r\n\t\t\t\t<el-table-column label=\"序号\" width=\"70\" :resizable='true' :sortable='true' align=\"left\" header-align=\"left\">\r\n\t\t\t\t\t<template #default=\"scope\">{{ scope.$index + 1}}</template>\r\n\t\t\t\t</el-table-column>\r\n\t\t\t\t<el-table-column\r\n\t\t\t\t\t :resizable='true' \r\n\t\t\t\t\t :sortable='true' \r\n\t\t\t\t\t align=\"left\" \r\n\t\t\t\t\t header-align=\"left\"\r\n\t\t\t\t\t prop=\"leixing\"\r\n\t\t\t\t\tlabel=\"类型\">\r\n\t\t\t\t\t<template #default=\"scope\">\r\n\t\t\t\t\t\t{{scope.row.leixing}}\r\n\t\t\t\t\t</template>\r\n\t\t\t\t</el-table-column>\r\n\t\t\t\t<el-table-column label=\"操作\" width=\"300\" :resizable='true' :sortable='true' align=\"left\" header-align=\"left\">\r\n\t\t\t\t\t<template #default=\"scope\">\r\n\t\t\t\t\t\t<el-button type=\"info\" v-if=\" btnAuth('leixing','查看')\" @click=\"infoClick(scope.row.id)\">详情</el-button>\r\n\t\t\t\t\t</template>\r\n\t\t\t\t</el-table-column>\r\n\t\t\t</el-table>\r\n\t\t\t<el-pagination \r\n\t\t\t\tbackground\r\n\t\t\t\t:layout=\"layouts.join(',')\"\r\n\t\t\t\t:total=\"total\" \r\n\t\t\t\t:page-size=\"listQuery.limit\"\r\n\t\t\t\tprev-text=\"上一页\"\r\n\t\t\t\tnext-text=\"下一页\"\r\n\t\t\t\t:hide-on-single-page=\"false\"\r\n\t\t\t\t:style='{\"padding\":\"0\",\"margin\":\"20px 0 0\",\"whiteSpace\":\"nowrap\",\"color\":\"#333\",\"textAlign\":\"center\",\"width\":\"100%\",\"fontWeight\":\"500\"}'\r\n\t\t\t\t@size-change=\"sizeChange\"\r\n\t\t\t\t@current-change=\"currentChange\" \r\n\t\t\t\t@prev-click=\"prevClick\"\r\n\t\t\t\t@next-click=\"nextClick\"  />\r\n\t\t</div>\r\n\t\t<formModel ref=\"formRef\" @formModelChange=\"formModelChange\"></formModel>\r\n\t</div>\r\n</template>\r\n<script setup>\r\n\timport axios from 'axios'\r\n\timport {\r\n\t\treactive,\r\n\t\tref,\r\n\t\tgetCurrentInstance,\r\n\t\tnextTick,\r\n\t\tonMounted,\r\n\t\twatch,\r\n\t} from 'vue'\r\n\timport {\r\n\t\tuseRoute,\r\n\t\tuseRouter\r\n\t} from 'vue-router'\r\n\timport {\r\n\t\tElMessageBox\r\n\t} from 'element-plus'\r\n\tconst context = getCurrentInstance()?.appContext.config.globalProperties;\r\n\timport formModel from './formModel.vue'\r\n\t\r\n\t//基础信息\r\n\tconst tableName = 'leixing'\r\n\tconst formName = '类型'\r\n\tconst route = useRoute()\r\n\t//基础信息\r\n\tonMounted(()=>{\r\n\t})\r\n\t//列表数据\r\n\tconst list = ref(null)\r\n\tconst table = ref(null)\r\n\tconst listQuery = ref({\r\n\t\tpage: 1,\r\n\t\tlimit: 20,\r\n\t\tsort: 'id',\r\n\t\torder: 'desc'\r\n\t})\r\n\tconst searchQuery = ref({})\r\n\tconst selRows = ref([])\r\n\tconst listLoading = ref(false)\r\n\tconst listChange = (row) =>{\r\n\t\tnextTick(()=>{\r\n\t\t\ttable.value.clearSelection()\r\n\t\t\ttable.value.toggleRowSelection(row)\r\n\t\t})\r\n\t}\r\n\t//列表\r\n\tconst getList = () => {\r\n\t\tlistLoading.value = true\r\n\t\tlet params = JSON.parse(JSON.stringify(listQuery.value))\r\n\t\tparams['sort'] = 'id'\r\n\t\tparams['order'] = 'desc'\r\n\t\tif(searchQuery.value.leixing&&searchQuery.value.leixing!=''){\r\n\t\t\tparams['leixing'] = '%' + searchQuery.value.leixing + '%'\r\n\t\t}\r\n\t\tcontext?.$http({\r\n\t\t\turl: `${tableName}/page`,\r\n\t\t\tmethod: 'get',\r\n\t\t\tparams: params\r\n\t\t}).then(res => {\r\n\t\t\tlistLoading.value = false\r\n\t\t\tlist.value = res.data.data.list\r\n\t\t\ttotal.va